@model ControleContas.Models.Usuario

<h2>Login</h2>
<form asp-action="Login" method="post">
    <div>
        <label>Email:</label>
        <input type="email" asp-for="Email" required />
    </div>
    <div>
        <label>Senha:</label>
        <input type="password" asp-for="Senha" required />
    </div>
    <button type="submit">Entrar</button>
</form>
